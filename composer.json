{
    "name": "jin2chen/yii-validator",
    "type": "library",
    "description": "validators from nest validation.",
    "keywords": [
        "yii3",
        "validator",
        "nest"
    ],
    "homepage": "https://www.jinchen.me/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jin2chen/yii-validator/issues?state=open",
        "source": "https://github.com/jin2chen/yii-validator"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/validator": "3.0.x-dev"
    },
    "require-dev": {
        "phpro/grumphp-shim": "^1.3",
        "phpunit/phpunit": "^9.5",
        "psalm/phar": "^5.6",
        "spatie/phpunit-watcher": "^1.23",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "jin2chen\\YiiValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jin2chen\\YiiValidator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction --colors",
        "test-watch": "phpunit-watcher watch"
    }
}
